{
  "buildCommand": "npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/expire-bookings",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/complete-bookings",
      "schedule": "0 1 * * *"
    }
  ]
}
