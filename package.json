{
  "name": "driveway-rental",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=development && node index.js",
    "start": "node index.js",
    "build": "cd frontend && npm ci && npm run build && cd .. && node -e \"const fs=require('fs');const path=require('path');if(fs.existsSync('public'))fs.rmSync('public',{recursive:true});fs.cpSync('frontend/dist','public',{recursive:true});\"",
    "migrate": "node scripts/migrate.js",
    "migrate:prod": "NODE_ENV=production node scripts/migrate.js",
    "add-indexes": "node scripts/add-performance-indexes.js",
    "remove-indexes": "node scripts/add-performance-indexes.js down",
    "optimize-db": "npm run add-indexes",
    "test-performance": "node scripts/performance-test.js",
    "postinstall": "echo 'Skipping migration - using Supabase'",
    "check-env": "node scripts/check-render-env.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:backend": "jest",
    "lint:backend": "eslint .",
    "lint:backend:fix": "eslint . --fix",
    "format:backend": "prettier --write \"./**/*.{js,json,css,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-geocoder": "^4.4.1",
    "pg": "^8.16.3",
    "redis": "^4.6.12",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "puppeteer": "^24.26.1"
  }
}
